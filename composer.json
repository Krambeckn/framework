{
    "name"       : "netforcews/framework",
    "description": "NetForce Framework by Laravel",
    "keywords"   : ["framework", "php", "netforce", "laravel"],
    "license"    : "GPL v.3",
    "authors"    : [
        {
            "name" : "Bruno Gon√ßalves",
            "email": "bugotech@gmail.com"
        }
    ],
    "require"    : {
        "laravel/framework"                  : "5.2.*",
        "davejamesmiller/laravel-breadcrumbs": "^3.0"
    },

    "replace": {
        "netforcews/support"   : "self.version",
        "netforcews/validation": "self.version",
        "netforcews/database"  : "self.version",
        "netforcews/io"        : "self.version",
        "netforcews/http"      : "self.version"
    },

    "require-dev": {
        "mockery/mockery": "~0.9.2",
        "pda/pheanstalk": "~3.0",
        "phpunit/phpunit": "~4.1",
        "predis/predis": "~1.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },

    "autoload": {

        "psr-4": {
            "NetForceWS\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },

    "minimum-stability": "dev"
}
